modules = ["web", "nodejs-20", "python-3.12"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 server.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 32783
externalPort = 3001

[[ports]]
localPort = 32993
externalPort = 3002

[[ports]]
localPort = 33853
externalPort = 3003

[[ports]]
localPort = 40083
externalPort = 5173

[[ports]]
localPort = 42689
externalPort = 3000

[[ports]]
localPort = 43971
externalPort = 6000

[[ports]]
localPort = 44607
externalPort = 5000

[[ports]]
localPort = 45659
externalPort = 4200

[deployment]
deploymentTarget = "autoscale"
run = ["python3", "server.py"]
